{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "^/$", "dest": "./index.html" },
    {
      "src": "./api/(.*)",
      "dest": "./server.js"
    },
    { "src": "./css.css", "dest": "./css.css" },
    { "src": "./script.js", "dest": "./script.js" },
    { "src": "./api.js", "dest": "./api.js" },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "./index.html"
    }
  ]
}
